{
    "ansible_role_name": "My New Ansible Role",
    "ansible_role_slug": "{{ cookiecutter.ansible_role_name|lower|replace(' ', '-') }}",
    "ansible_role_dependencies": "",
    "ansible_role_description": "Ansible role to install and configure {{ cookiecutter.ansible_role_slug }}",
    "ansible_role_license": [
        "MIT",
        "No License",
        "other"
    ],
    "ansible_role_minimal_version": "2.8",
    "ansible_role_github_branch": "main",
    "ansible_role_platforms": "Debian:stretch,buster;Ubuntu:focal,bionic;EL:7,8",
    "ansible_role_tags": "",
    "author_email": "user@email.tld",
    "author_github_username": "github_username",
    "ansible_role_repository": "{{ cookiecutter.author_github_username }}/ansible-role-{{ cookiecutter.ansible_role_name }}",
    "author_name": "Full Name",
    "company_name": "Foobar Inc.",
    "company_url": "https://foo.bar",
    "year": "{% now 'utc', '%Y' %}",
    "_extensions": [
        "jinja2_time.TimeExtension",
        "cookiecutter.extensions.SlugifyExtension"
    ],
    "_copy_without_render": [
      ".github/workflows/ci.yml",
      ".github/workflows/repo-config.yml",
      "molecule/default/create.yml",
      "molecule/default/destroy.yml",
      "molecule/default/Dockerfile.j2"
    ]
}
